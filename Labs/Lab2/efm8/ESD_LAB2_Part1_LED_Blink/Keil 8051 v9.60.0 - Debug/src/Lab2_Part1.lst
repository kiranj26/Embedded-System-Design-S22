                        127     ; Details      -
                        128     ; The assembly code triggers the ISR from timer 0 and toggles the LED at P1.0 at about 1
                               .19Hz.
                        129     ; It also uses a unused port pin as logic high as interrupt occurs and logic low as inte
                               rrupt is ended.
                        130     
000000                  131     ORG 0000H
000000 020000     F     132     LJMP MAIN       ; Jump to MAIN function
                        133     
00000B                  134     ORG 000BH       ; Interrupt Service Routine (ISR) for Timer 0
00000B 020000     F     135     LJMP ISR_TMR    ; Jump to ISR_TMR for Interrupt Service Routine
                        136     
000040                  137     ORG 0040H
                        138     
000040                  139     MAIN:
                        140               ;MOV P1, #11111111 ; Initialize P1 with all pins set to logic high (LED off)
                        141           ;MOV P1, #11110111 ; PIN 3 High
000040 E590             142           MOV A, P1
000042 758901           143           MOV TMOD,#0X01    ; Timer 0, mode 1 (16-bit timer)
000045 75A882           144               MOV IE,#0X82  ; Enable Timer 0 interrupt and global interrupt
                        145     
000048 30930F           146           JNB P1.3, SET_NEW_TIME ; Check if P1.3 (input pin) is logic high, and set timer to
                                new frequency if so
00004B 758C17           147               MOV TH0,#0X17 ; Higher byte of Timer 0
00004E 758A46           148               MOV TL0,#0X46 ; Lower byte of Timer 0
000051 75F006           149               MOV B,#6         ; Set register B to 6 for loop
000054 8000             150           JMP START_TIMER
                        151     
000056                  152     START_TIMER:
000056 D28C             153           SETB TR0     ; Start Timer 0
000058 0100       F     154     HERE: AJMP HERE     ; Loop indefinitely until interrupt occurs
                        155     
00005A                  156     SET_NEW_TIME:
00005A 758C93           157           MOV TH0,#0X93     ; Higher byte of Timer 0
00005D 758A5B           158               MOV TL0,#0X5B ; Lower byte of Timer 0
000060 7812             159           MOV R0, #18    ; Set R0 to 18 for loop
000062 80F2             160               JMP START_TIMER ; Restart timer with new frequency
                        161     
000064                  162     ISR_TMR:
000064 B292             163                     CPL P1.2            ; Toggle an unused pin to indicate interrupt
000066 309314           164                     JNB P1.3 ,LOWER_FRQ ; If P1.3 is high, go to the lower frequency branch
000069 209300           165                 JB P1.3, NORMAL
00006C                  166     NORMAL:
00006C D5F01D           167                DJNZ B,LOOP  ; Decrement B and wait until B is zero
00006F 758C17           168                MOV TH0,#0X17        ; Higher byte of Timer 0
000072 758A46           169                MOV TL0,#0X46        ; Lower byte of Timer 0
000075 75F006           170                MOV B,#6         ; Set register B to 6 for loop
000078 B290             171                CPL P1.0         ; Toggle the LED at P1.0
00007A B292             172                CPL P1.2         ; Toggle the unused pin to indicate interrupt
AX51 MACRO ASSEMBLER  LAB2_PART1                                                            02/25/23 17:35:13 PAGE     4

00007C 32               173                RETI                 ; Return from Interrupt
                        174     
00007D                  175     LOWER_FRQ:
00007D D80D             176                DJNZ R0,LOOP     ; Decrement R0 and wait until R0 is zero
00007F 758C94           177                MOV TH0,#0X94        ; Higher byte of Timer 0 for lower frequency
000082 758A5B           178                MOV TL0,#0X5B        ; Lower byte of Timer 0 for lower frequency
000085 7812             179                MOV R0,#18       ; Set R0 to 18 for loop
000087 B290             180                CPL P1.0         ; Toggle the LED at P1.0
000089 B292             181                CPL P1.2         ; Toggle the unused pin to indicate interrupt
00008B 32               182                RETI                 ; Return from Interrupt
                        183     
